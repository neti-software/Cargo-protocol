{
  "name": "cargo-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "build:dev": "nest build && pm2 start pm2.yaml",
    "build:prod": "nest build && NODE_PORT=3003 pm2 start pm2.yaml -n nest-demo --env production",
    "console:dev": "ts-node -r tsconfig-paths/register src/console.ts",
    "console": "node dist/src/console.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/database/config/ormconfig.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert",
    "typeorm:create": "npm run typeorm migration:create -- -n"
  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@celo-tools/celo-ethers-wrapper": "0.0.5",
    "@nestjs/common": "7.6.15",
    "@nestjs/core": "7.6.15",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.0.0",
    "@nestjs/platform-express": "7.6.15",
    "@nestjs/schedule": "1.0.1",
    "@nestjs/swagger": "4.8.0",
    "@typechain/ethers-v5": "7.0.1",
    "@typechain/hardhat": "2.2.0",
    "@types/lodash": "^4.14.182",
    "@uniswap/v3-sdk": "3.3.1",
    "apollo-boost": "^0.4.9",
    "axios": "0.21.4",
    "bcrypt": "5.0.1",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "commander": "7.2.0",
    "cross-fetch": "^3.1.5",
    "dotenv": "10.0.0",
    "ethereum-block-by-date": "^1.4.2",
    "ethers": "5.4.2",
    "graphql": "^14.7.0",
    "hardhat": "2.2.0",
    "hardhat-deploy": "0.8.9",
    "hardhat-typechain": "0.3.5",
    "helmet": "4.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "morgan": "1.10.0",
    "mysql": "2.18.1",
    "nestjs-console": "5.0.1",
    "node-apollo-client": "^0.1.7",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pm2": "5.1.0",
    "react-apollo": "^3.1.5",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.3.0",
    "swagger-ui-express": "4.1.6",
    "typechain": "5.1.2",
    "typeorm": "0.2.33"
  },
  "devDependencies": {
    "@nestjs/cli": "7.6.0",
    "@nestjs/config": "1.0.1",
    "@nestjs/schematics": "7.3.0",
    "@nestjs/testing": "7.6.15",
    "@nestjs/typeorm": "8.0.1",
    "@nomiclabs/hardhat-ethers": "2.0.2",
    "@nomiclabs/hardhat-etherscan": "2.1.3",
    "@nomiclabs/hardhat-waffle": "2.0.1",
    "@types/bcrypt": "5.0.0",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.22",
    "@types/node": "^14.14.36",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.33",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@ubeswap/sdk": "^2.1.1",
    "@walletconnect/web3-provider": "^1.7.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "26.6.3",
    "prettier": "^2.3.2",
    "supertest": "6.1.3",
    "ts-jest": "26.5.4",
    "ts-loader": "8.0.18",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.4",
    "typescript-transform-paths": "3.1.0",
    "web3-utils": "^1.6.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
